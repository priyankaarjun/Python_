class laptop:
  price = 0
  processor = " "
  ram = " "

hp = laptop()
dell = laptop()
lenova = laptop()

hp.price = 40000
hp.processor = "i5"
hp.ram = "8GB"

dell.price = 50000
dell.processor = "i7"
dell.ram = "16GB"

lenova.price = 60000
lenova.processor = "i9"
lenova.ram = "32GB"

print(hp.price)
print(lenova.ram)
